{
  "name": "tailwindcss-material",
  "version": "1.0.0",
  "description": "Material plugin for tailwindcss",
  "main": "index.js",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "jest",
    "release": "HUSKY=0 standard-version -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahaasler/tailwindcss-material.git"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "plugin",
    "colors",
    "material"
  ],
  "author": "Adrian Haasler Garc√≠a <dev@ahaasler.com> (https://ahaasler.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahaasler/tailwindcss-material/issues"
  },
  "homepage": "https://github.com/ahaasler/tailwindcss-material#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "pinst": "^2.1.1",
    "standard-version": "^9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "tailwindcss": "2.0.0"
  }
}
